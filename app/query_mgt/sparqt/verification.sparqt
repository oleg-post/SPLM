    "reqs" : {
        "_CMT_": "Экспертиза требований. Страница /expertise_report.php",
        "#VARS":{},
        "#TXT":"select ?doc ?doc_lbl ?doc_cls ?itemver ?itemver_lbl ?itemver_cls ?txt ?status (count(?prdoc) as ?doc_qnt) {
                    ?itemver onto:hasRecordOfVerification ?record .
                    ?record onto:hasAttribute ?att_v .
                    ?att_v a onto:Verification .
                    ?att_v onto:hasAttributeValue/rdfs:label ?status .
                    ?item onto:hasVersion ?itemver .
                    ?item rdfs:label ?itemver_lbl .
                    ?itemver rdf:type ?itemver_cls .
                    ?itemver rdfs:label ?txt .
                    ?itemver onto:isFromDoc ?doc .
                    ?doc rdfs:label ?doc_lbl .
                    ?doc a ?doc_cls .
                    Optional {?itemver onto:isTracedTo ?prdoc . }
                } group by ?doc ?doc_lbl ?doc_cls ?itemver ?itemver_lbl ?itemver_cls ?txt ?status order by ?doc_lbl ?itemver_lbl"
    },
    "reqs_Contract" : {
        "_CMT_": "Экспертиза требований Контракта. Страница /expertise_report.php",
        "#VARS":{"TZT":{"mark":"#{TZT}","default":""}},
        "#TXT":"SELECT ?tz ?tz_cls ?tz_lbl ?rc_lbl ?work_ver ?work_ver_cls ?work_ver_lbl ?comp_lbl ?exp_lbl ?comment_val ?result_val (count(?sec) as ?cnt ) {
                    ?tema onto:hasAttributeValue/rdfs:label \"#{TZT}\" .
                    ?tema a onto:Topic .
                    ?tz onto:hasAttribute ?tema .
                    ?tz rdfs:label ?tz_lbl .
                    ?tz a ?tz_cls .
                    ?tz onto:hasDocumentationPlan ?dp .
                    ?dp onto:hasRecord ?rc .
                    ?rc rdfs:label ?rc_lbl .
                    ?rc onto:hasWorkResult ?work .
                    ?work rdfs:label ?work_ver_lbl .
                    ?work onto:madeBy ?comp .
                    ?comp rdfs:label ?comp_lbl .
                    ?work onto:hasVersion ?work_ver .
                    ?work_ver a ?work_ver_cls .
                    ?work_ver onto:hasRecordOfVerification ?recver .
                    ?recver onto:isVerifiedBy ?exp .
                    ?exp rdfs:label ?exp_lbl .
                    ?recver onto:hasAttribute ?comment .
                    ?comment a onto:DocExplanatoryNote .
                    ?comment onto:hasAttributeValue/rdfs:label ?comment_val .
                    ?recver onto:hasAttribute ?result .
                    ?result a onto:Verification .
                    ?result onto:hasAttributeValue/rdfs:label ?result_val .
                    ?recver onto:hasSection ?sec .
                } group by ?tz ?tz_cls ?tz_lbl ?rc_lbl ?work_ver ?work_ver_cls ?work_ver_lbl ?comp_lbl ?exp_lbl ?comment_val ?result_val order by ?tz_lbl ?rc_lbl"
    },
    "reqs_STUK" : {
        "_CMT_": "Экспертиза требований STUK. Страница /expertise_report.php",
        "#VARS":{},
        "#TXT":"select ?doc ?doc_lbl ?doc_cls ?sequence ?itemver ?itemver_lbl ?itemver_cls ?txt ?status (count(?prdoc) as ?doc_qnt) {
                    ?itemver onto:hasSectionOfVerification ?sec .
                    ?itemver onto:hasAttribute ?seq .
                    ?seq a onto:SequenceNumber .
                    ?seq onto:hasAttributeValue ?val .
                    ?val onto:value ?sequence .
                    ?sec onto:hasAttribute ?att_s .
                    ?att_s a onto:Verification .
                    ?att_s onto:hasAttributeValue ?att_s_val .
                    ?att_s_val rdfs:label ?status .
                    ?item rdfs:label ?itemver_lbl .
                    ?item onto:hasVersion ?itemver .
                    ?itemver rdf:type ?itemver_cls .
                    OPTIONAL{ ?itemver rdfs:label ?txt . }
                    ?itemver onto:isFromDoc ?doc .
                    ?doc rdfs:label ?doc_lbl .
                    ?doc a onto:RegulatoryDocument .
                    ?doc a ?doc_cls .
                    Optional {?itemver onto:isTracedTo ?prdoc . }
                } group by ?doc ?doc_lbl ?doc_cls ?sequence ?itemver ?itemver_lbl ?itemver_cls ?txt ?status order by ?doc_lbl ?sequence"
    },
