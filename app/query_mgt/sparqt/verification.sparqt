        "#VARS":{"CLS":{"mark":"#{cls}","default":"onto:Document"}},
        "#TXT":"select ?docver ?docver_cls ?docver_lbl ?doc_name (substr(?vr,1,1) as ?rev) (substr(?vr,3) as ?ver) ?r_lbl ?com ?com_cls ?com_lbl ?exp_lbl (count(?sec) as ?req_num) {
                    ?rep a onto:DocVerificationReport .
                    ?rep onto:hasRecord ?rec .
                    ?rec onto:isVerifiedBy ?exp .
                    ?exp rdfs:label ?exp_lbl .
                    ?rec onto:hasAttribute ?r_att .
                    ?r_att a onto:Verification .
                    ?r_att onto:hasAttributeValue ?r_val .
                    ?r_val rdfs:label ?r_lbl .
                    ?docver onto:hasRecordOfVerification ?rec .
                    ?docver onto:latestVersion xsd:true .
                    OPTIONAL {
                        ?rec onto:hasSection ?sec .
                        ?itemver onto:hasSectionOfVerification ?sec .
                        ?item onto:hasVersion ?itemver .
                        ?item a onto:Requirement .
                    } .
                    ?doc onto:hasVersion ?docver .
                    ?doc onto:madeBy ?com .
                    ?doc rdfs:label ?docver_lbl .
                    ?doc rdf:type #{cls} .
                    ?docver rdfs:label ?vr .
                    ?docver a ?docver_cls .
                    ?doc onto:hasAttribute ?d_att .
                    ?d_att a onto:DocumentName .
                    ?d_att onto:hasAttributeValue ?d_val .
                    ?d_val rdfs:label ?doc_name .
                    ?com a ?com_cls .
                    ?com rdfs:label ?com_lbl .
                } group by ?docver ?docver_cls ?docver_lbl ?doc_name ?vr ?r_lbl ?com ?com_cls ?com_lbl ?exp_lbl"
    },
    "docs_cls_cnt" : {
        "_CMT_": "Экспертиза документов. Страница /verification",
        "#VARS":{"CLS":{"mark":"#{cls}","default":"onto:Document"}},
        "#TXT":"select (count(DISTINCT ?docver) AS ?cnt) {
                    ?rep a onto:DocVerificationReport .
                    ?rep onto:hasRecord ?rec .
                    ?rec onto:hasAttribute ?r_att .
                    ?r_att a onto:Verification .
                    ?docver onto:hasRecordOfVerification ?rec .
                    ?docver onto:latestVersion xsd:true .
                    OPTIONAL {
