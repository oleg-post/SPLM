    "mrx_cls" : {
        "_CMT_": "Трассировка требований. От требования к документам. Страница /tracing.php",
        "#VARS":{"CLS":{"mark":"#{cls}","default":"onto:Document"}},
        "#TXT":"SELECT DISTINCT ?ver ?ver_lbl ?ver_cls ?verlbl (group_concat(?doc_lbl; separator=\"; \") as ?doc_list) {
