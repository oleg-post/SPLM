    "reqs_by_orgs" : {
        "_CMT_": "Распределение требований по исполнителям",
        "#VARS":{"URI":{"mark":"#{URI}","default":""},"lang":{"mark":"#{lang}","default":"RU"}},
        "#TXT":"select distinct ?org_lbl (count(distinct ?item) as ?total_req ) (count(distinct ?ver) as ?traced_req) (count(distinct ?sec) as ?verified) (count(distinct ?prdoc) as ?planned_doc ) (count(distinct ?namew_val) as ?work_doc ) {                                               
                    ?item a onto:Requirement .
                    ?item onto:hasReqImplementer ?org .
                    ?org rdfs:label ?org_lbl . 
                    Optional {?item onto:hasVersion ?ver .
                    ?ver onto:isTracedTo ?docver .
                    ?docver rdfs:label ?docver_lbl .}
                    Optional {?item onto:hasVersion ?ver2 .
                    ?ver2 onto:hasSectionOfVerification ?sec .
                    ?sec onto:hasAttribute ?s_att .
                    ?s_att a onto:Verification .
                    ?s_att onto:hasAttributeValue/rdfs:label ?ver_val . }
                    Optional {?prdoc onto:hasContractor ?org .
                    ?prdoc a ?prdoc_cls . filter(?prdoc_cls != onto:TZ ) }
                    Optional {?wdoc onto:madeBy ?org .
                    ?wdoc onto:hasAttribute ?namew .
                    ?namew a onto:DocumentName .
                    ?namew onto:hasAttributeValue ?namew_val .
                    ?wdoc rdfs:label ?wdoc_lbl .}
                } group by ?org_lbl ORDER BY ?org_lbl"
    },
