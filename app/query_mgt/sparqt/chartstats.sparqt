    "reqs_by_topic_pdocs" : {
        "_CMT_": "Статистика. График распределение по темам по отчетным документам",
        "#VARS":{"URI":{"mark":"#{URI}","default":""},"lang":{"mark":"#{lang}","default":"RU"}},
        "#TXT":"select ?tema_lbl  (count(distinct ?rslt) as ?doc_planned) {
                    ?tema a onto:Topic .
                    ?tema onto:hasAttributeValue/rdfs:label ?tema_lbl .
                    ?tz onto:hasAttribute ?tema .
                    ?item onto:isCreatedIn ?tz .
                    ?item a onto:Requirement .
                    Optional {?tz onto:hasDocumentationPlan ?docplan .
                    ?docplan onto:hasRecord/onto:hasPlanResult ?rslt .}
                } group by ?tema_lbl"
    },
    "reqs_by_topic_wdocs" : {
        "_CMT_": "Статистика. График распределение по темам по разработанным документам",
        "#VARS":{"URI":{"mark":"#{URI}","default":""},"lang":{"mark":"#{lang}","default":"RU"}},
