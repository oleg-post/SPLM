{
    "reqs_by_topic" : {
        "_CMT_": "Заглушка",
        "#VARS":{"URI":{"mark":"#{URI}","default":""},"lang":{"mark":"#{lang}","default":"RU"}},
        "#TXT":"select ?tema_lbl (count(distinct ?item) as ?total_req ) (count(distinct ?org) as ?companies) (count(distinct ?ver) as ?traced_req) (count(distinct ?sec) as ?verified) {
                    ?tema a onto:Topic .
                    ?tema onto:hasAttributeValue/rdfs:label ?tema_lbl .
                    ?tz onto:hasAttribute ?tema .
                    ?item onto:isCreatedIn ?tz .
                    ?item a onto:Requirement .
                    ?item onto:hasReqImplementer ?org .
                    Optional {?item onto:hasVersion ?ver .
                    ?ver onto:isTracedTo ?docver .
                    ?docver rdfs:label ?docver_lbl .}
                    Optional {?item onto:hasVersion ?ver2 .
                    ?ver2 onto:hasSectionOfVerification ?sec .
                    ?sec onto:hasAttribute ?s_att .
                    ?s_att a onto:Verification .
                    ?s_att onto:hasAttributeValue/rdfs:label ?ver_val . }
                } group by ?tema_lbl"
    },
    "reqs_by_topic_total" : {
        "_CMT_": "по всеЗаглушкам требованиям",
        "#VARS":{"URI":{"mark":"#{URI}","default":""},"lang":{"mark":"#{lang}","default":"RU"}},
        "#TXT":"select ?tema_lbl (count(distinct ?item) as ?total_req ) {
                    ?tema a onto:Topic .
                    ?tema onto:hasAttributeValue/rdfs:label ?tema_lbl .
                    ?tz onto:hasAttribute ?tema .
                    ?item onto:isCreatedIn ?tz .
                    ?item a onto:Requirement .
                } group by ?tema_lbl"
    },
